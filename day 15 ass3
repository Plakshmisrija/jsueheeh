#include <stdio.h>
int main()
 {
    int x, y;
    float result = 1.0;
    printf("Enter base number: ");
    scanf("%d", &x);
    printf("Enter exponent number: ");
    scanf("%d", &y);

    while (y != 0)
	 {
        result *= x;
        --y;
    }
    printf("the answer is=%f", result);
    return 0;
}
